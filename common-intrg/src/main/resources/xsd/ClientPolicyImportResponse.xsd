<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">

	<xs:element name="PolicyImport" type="PolicyImportWrapper">
		<xs:annotation>
			<xs:documentation>Policy import.</xs:documentation>
		</xs:annotation>
	</xs:element>

	<xs:complexType name="PolicyImportWrapper">
		<xs:sequence>
			<xs:element name="policy" type="Policy" minOccurs="1"
				maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Policy Information containing Units,
						Participants, Coverages, Limits and Deductibles.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="persons" type="personList" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Individual participants.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="organizations" type="orgList" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Organization (Vendor, Bank, Agency etc)
						Participants
					</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="orgList">
		<xs:sequence>
			<xs:element name="organization" type="Organization"
				minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Organization.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="personList">
		<xs:sequence>
			<xs:element name="person" type="Person" minOccurs="0"
				maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Person.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="Profile">
		<xs:sequence>
			<xs:element name="profileName" type="xs:string" minOccurs="1"
				maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Profile name.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="externalSourceId" type="xs:string"
				minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>External identifier.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="profileResponseText" type="xs:string"
				minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Profile value.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="Deductibledetail">
		<xs:sequence>
			<xs:element name="deductibleAmount" type="xs:double"
				minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Deductible amount.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="deductibleTypeCode" type="xs:string"
				minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Deductible detail type
						Water Back Up
						Windstorm or
						Hail
						Identity Fraud
						Hull
						Refrigerated Products
						Earthquake Assessment
						Earthquake
						Flat
						PIP
						Liability
						Combined Single Limit
						Boat
						Workers Comp
						Inland Marine
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="externalSourceId" type="xs:string"
				minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>External identifier.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="Deductible">
		<xs:sequence>
			<xs:element name="deductibleTypeCode" type="xs:string"
				minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Deductible type
						Single
						Multiple
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="effectiveDate" type="xs:date" minOccurs="1"
				maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Effective date.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="expiryDate" type="xs:date" minOccurs="0"
				maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Expiry date.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="externalSourceId" type="xs:string"
				minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>External identifier.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Deductibledetails" type="DeductibleDetailList"
				minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Deductibles.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="DeductibleDetailList">
		<xs:sequence>
			<xs:element name="deductibledetail" type="Deductibledetail"
				minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Deductible detail.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="Limitdetail">
		<xs:sequence>
			<xs:element name="externalSourceId" type="xs:string"
				minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>External identifier.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="limitAmount" type="xs:double" minOccurs="1"
				maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Limit amount.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="limitTypeCode" type="xs:string"
				minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Limit detail types
						Each Person
						Each Occurrence
						Agreed Value
						Medical Pay
						Per Day
						Physical Damage
						Liability
					</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="Limit">
		<xs:sequence>
			<xs:element name="effectiveDate" type="xs:date" minOccurs="1"
